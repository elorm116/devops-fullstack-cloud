{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(http_request_duration_ms_count[5m])",
          "legendFormat": "{{method}} {{route}} ({{code}})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 15 }
        }
      }
    },
    {
      "title": "Request Latency p50 / p95 / p99",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_ms_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_ms_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_ms_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "HTTP Status Codes (rate/5m)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum by (code) (rate(http_request_duration_ms_count[5m]))",
          "legendFormat": "{{code}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "bars", "fillOpacity": 50, "stacking": { "mode": "normal" } }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "200" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "401" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "500" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "title": "Error Rate (4xx + 5xx)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_ms_count{code=~\"4..|5..\"}[5m])) / sum(rate(http_request_duration_ms_count[5m])) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "App Up",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "up{job=\"myjs-app\"}",
          "legendFormat": "up",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ]
        }
      }
    },
    {
      "title": "Memory Usage (RSS)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=\"myjs-app\"}",
          "legendFormat": "RSS",
          "refId": "A"
        },
        {
          "expr": "nodejs_heap_size_used_bytes{job=\"myjs-app\"}",
          "legendFormat": "Heap Used",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 15 }
        }
      }
    },
    {
      "title": "CPU Usage",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=\"myjs-app\"}[5m])",
          "legendFormat": "CPU seconds/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 15 }
        }
      }
    },
    {
      "title": "Event Loop Lag",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus-main-001" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_seconds{job=\"myjs-app\"}",
          "legendFormat": "Event Loop Lag",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 15 }
        }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["api", "nodejs", "blog"],
